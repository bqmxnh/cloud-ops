ENV=production

# Model Configuration
# AUTO_DETECT_MODEL=true: Automatically detect which model is in Production stage (recommended)
# AUTO_DETECT_MODEL=false: Use MODEL_NAME specified below
AUTO_DETECT_MODEL=true

# MODEL_NAME: Only used if AUTO_DETECT_MODEL=false
# Available models: "ARF Baseline Model", "KNN Baseline Model", "HAT Baseline Model"
MODEL_NAME=ARF Baseline Model
MODEL_BUCKET=arf-ids-model-bucket
MODEL_VERSION=v1.0
AWS_DEFAULT_REGION=us-east-1
